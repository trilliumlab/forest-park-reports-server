{
  "name": "forest-park-reports-server",
  "version": "0.1.0",
  "description": "A Typescript Project.",
  "type": "module",
  "engines": {
    "node": ">= 20 <21"
  },
  "devDependencies": {
    "@types/app-root-path": "~1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "~20",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "~6.2",
    "@typescript-eslint/parser": "~6.2",
    "concurrently": "~8.2",
    "eslint": "~8.46",
    "eslint-config-prettier": "~8.10",
    "nodemon": "~3.0",
    "prettier": "~3.0",
    "rimraf": "~5.0",
    "tsutils": "~3.21",
    "typescript": "~5.1"
  },
  "scripts": {
    "start": "npm run build && npm run run | pino-pretty -t -c",
    "start:watch": "concurrently \"npm run build:watch\" \"nodemon build/server.js | pino-pretty -t -c\"",
    "clean": "rimraf coverage build tmp",
    "prebuild:release": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "run": "node build/server.js",
    "lint": "eslint . --ext .ts --ext .mts",
    "prettier": "prettier --config .prettierrc --write ."
  },
  "author": "Elliot Nash <elliotnash@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/multipart": "^7.7.3",
    "@fastify/static": "^6.10.2",
    "app-root-path": "~3.1",
    "fastify": "~4.21",
    "fs-extra": "^11.1.1",
    "fast-xml-parser": "^4.2.7",
    "pg": "^8.11.2",
    "pino": "~8.15",
    "pino-pretty": "~10.2",
    "tslib": "~2.6",
    "uuid": "^9.0.0"
  },
  "volta": {
    "node": "16.13.0"
  }
}
