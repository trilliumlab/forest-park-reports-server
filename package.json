{
  "name": "forest-park-reports-server",
  "version": "0.1.0",
  "description": "A Typescript Project.",
  "type": "module",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/app-root-path": "~1.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "~16",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "~5.26",
    "@typescript-eslint/parser": "~5.26",
    "concurrently": "~7.2",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "nodemon": "~2.0",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "tsutils": "~3.21",
    "typescript": "~4.7"
  },
  "scripts": {
    "start": "npm run build && npm run run | pino-pretty -t -c",
    "start:watch": "concurrently \"npm run build:watch\" \"nodemon build/server.js | pino-pretty -t -c\"",
    "clean": "rimraf coverage build tmp",
    "prebuild:release": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "run": "node build/server.js",
    "lint": "eslint . --ext .ts --ext .mts",
    "prettier": "prettier --config .prettierrc --write ."
  },
  "author": "Elliot Nash <elliotnash@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "~3.0",
    "fastify": "~4.2",
    "fs-extra": "^10.1.0",
    "pg": "^8.7.3",
    "pino": "~8.1",
    "pino-pretty": "~8.1",
    "tslib": "~2.4",
    "uuid": "^8.3.2"
  },
  "volta": {
    "node": "16.13.0"
  }
}
